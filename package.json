{
  "name": "react-task",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently -n REACT,PROXY -c blue,green \"npm run dev:client\" \"npm run dev:proxy\" ",
    "dev:client": "npm run format && cross-env NODE_ENV=development webpack-dev-server --open",
    "dev:proxy": "nodemon src/server/server.js",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "jest --coverage",
    "format": "prettier --write ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
